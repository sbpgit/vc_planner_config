{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/api/(.*)$",
      "target": "/$1",
      "destination": "VC_Planner_Config",
      "authenticationType": "xsuaa",
      "csrfProtection": false,
      "scope": {
        "GET": [
          "$XSAPPNAME.User",
          "$XSAPPNAME.admin"
        ],
        "PUT": [
          "$XSAPPNAME.User",
          "$XSAPPNAME.admin"
        ],
        "PATCH": [
          "$XSAPPNAME.User",
          "$XSAPPNAME.admin"
        ],
        "POST": [
          "$XSAPPNAME.User",
          "$XSAPPNAME.admin"
        ],
        "DELETE": [
          "$XSAPPNAME.User",
          "$XSAPPNAME.admin"
        ],
        "default": [
          "$XSAPPNAME.User",
          "$XSAPPNAME.admin"
        ]
      }
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "none"
    }
  ]
}
